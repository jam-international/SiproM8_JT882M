'#include "DefVar.h"



sub ()
	//CASO ESTREMO, L'operatore chiede di ripartire a cucire quando è in HOLD sull'ultimo punto
	// Verifica cucitura terminata
	if ( vq1952          >= vq1951          )
		// Movimento a posizione di home ( coordinate macchina )
		SyncVar( vb1003          )
		Delays ( 0.3 )
 		vb4805                      = 1 
		vq1962     = vq1717            
		if ( vq1918        < vq1962     )           // #M200717_1GF#
			vq1962     = vq1918       
		endif

		//ISO G1 X C1_QuoHomeX Y C1_QuoHomeY F C1_VelFeed

		vb37            = 1 		//spengo perni ranocchia


		fa[ 1        ] = vq3054                 
		fa[ 2        ] = vq3054                 
		ax[ 1        ] = vq1911     
		ax[ 2        ] = vq1912     
		Delays (0.1)

		//Daniele x fine test
		////********** Sono in HOLD e ho premuto START sull'ultimo punto **********************
		if(Vb4006                == 0  )


				Vb4350                         = 1 
				Vb4351                         = 1  
				vb302 = 1 

			WaitCond ( vb224                       == 1)

		else

			if(vb7948        == 0)
				if(vb7928        == 0 and vb2955       == 0 and Vb4382                         == 0  )
					Vb4382                         = 1 
					vb2004           = 0  
					 SyncVar( vb2003          )    


				endif


				vb12                    = 0  
				Vb4016                  = 1 
				vn11         = 0
				vn12         = 0
				vb7941      = 1
			endif




		endif

		vb36            = 1 	//apro retrattile
		vb37            = 1 	//tolgo ranocchia
		vb38            = 1 	//disattivo salto piedino


		gosub "Halt.obj"
	endif
endsub
